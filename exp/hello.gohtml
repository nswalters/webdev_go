<h1>Hello, {{.Name}}</h1>
<p>The weather today is: {{.Weather}}</p>
<em>The weather for the next couple of days...</em>
{{range $k, $v := .WeekWeather}}
    <p style="margin-left: 25px"><strong>{{$k}}:</strong> {{$v}}</p>
{{end}}
{{if .IsCool -}}
    <p>Is Cool</p>
{{else -}}
    <p>Is NOT Cool</p>
{{end}}